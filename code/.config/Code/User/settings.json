{
    "breadcrumbs.enabled": false,
    "debug.console.fontFamily": "JetBrainsMono NF",
    "dotnetAcquisitionExtension.enableTelemetry": false,
    "editor.bracketPairColorization.enabled": false,
    "editor.codeActionWidget.showHeaders": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorStyle": "line",
    "editor.experimentalInlineEdit.fontFamily": "JetBrainsMono NF",
    "editor.fontFamily": "JetBrainsMono NF",
    "editor.fontLigatures": true,
    "editor.fontVariations": true,
    "editor.fontWeight": "normal",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.guides.indentation": false,
    "editor.inlayHints.fontFamily": "JetBrainsMono NF",
    "editor.inlineSuggest.fontFamily": "JetBrainsMono NF",
    "editor.minimap.enabled": false,
    "editor.mouseWheelZoom": true,
    "editor.occurrencesHighlight": "off",
    "editor.stickyScroll.enabled": false,
    "editor.tabSize": 4,
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "files.autoSave": "onFocusChange",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "godot.csharp.executablePath": "${env:GODOT}",
    "oneDarkPro.bold": true,
    "symbols.hidesExplorerArrows": true,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.fontFamily": "JetBrainsMono NF",
    "terminal.integrated.mouseWheelZoom": true,
    "window.commandCenter": false,
    "window.customTitleBarVisibility": "auto",
    "window.restoreFullscreen": true,
    "window.title": " ",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 1,
    "workbench.activityBar.location": "default",
    "workbench.colorTheme": "One Dark Pro Darker",
    "workbench.editor.showTabs": "multiple",
    "workbench.iconTheme": "symbols",
    "workbench.layoutControl.enabled": false,
    "workbench.productIconTheme": "fluent-icons",
    "git.ignoreRebaseWarning": true,
    "github.copilot.enable": {
        "*": false
    }
}